{
  "name": "@lumina-dex/sdk",
  "version": "0.4.0",
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "Hebilicious"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lumina-DEX/lumina.git",
    "directory": "packages/sdk"
  },
  "scripts": {
    "lint": "bunx @biomejs/biome lint src/",
    "lint:fix": "bunx @biomejs/biome lint --write src/",
    "format": "dprint check",
    "format:fix": "dprint fmt --verbose",
    "build": "unbuild",
    "test": "echo 'test succesful'",
    "schema:zeko:get": "gql-tada generate schema 'https://devnet.zeko.io/graphql' --output './src/graphql/sequencer.graphql'",
    "schema:mina:get": "gql-tada generate schema 'https://api.minascan.io/node/devnet/v1/graphql' --output './src/graphql/mina.graphql'",
    "schema:mina:archive:get": "gql-tada generate schema 'https://api.minascan.io/archive/devnet/v1/graphql' --output './src/graphql/mina-archive.graphql'"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.mjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@aurowallet/mina-provider": "^1.0.6",
    "@gql.tada/vue-support": "^1.0.1",
    "@lumina-dex/contracts": "workspace:^",
    "@urql/exchange-retry": "^1.3.0",
    "@urql/vue": "^1.4.1",
    "@xstate/react": "^4.1.3",
    "@xstate/store": "^2.6.1",
    "comlink": "^4.4.2",
    "consola": "^3.4.0",
    "fflate": "^0.8.2",
    "gql.tada": "^1.8.10",
    "o1js": "^2.2.0",
    "urql": "^4.1.0",
    "vue": "^3.5.11",
    "xstate": "^5.18.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "latest",
    "magic-string": "^0.30.14",
    "rollup": "^4.27.4",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  }
}