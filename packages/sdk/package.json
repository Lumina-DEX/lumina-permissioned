{
	"name": "@lumina/sdk",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"build": "unbuild",
		"schema:zeko:get": "bunx get-graphql-schema https://devnet.zeko.io/graphql > ./src/graphql/sequencer.graphql",
		"schema:mina:get": "bunx get-graphql-schema https://api.minascan.io/node/devnet/v1/graphql > ./src/graphql/mina.graphql"
	},
	"files": ["dist"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		},
		"./vue": {
			"types": "./dist/vue.d.ts",
			"import": "./dist/vue.mjs"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@aurowallet/mina-provider": "^1.0.6",
		"@gql.tada/vue-support": "^1.0.1",
		"@lumina/contracts": "workspace:^",
		"@surma/rollup-plugin-off-main-thread": "^2.2.3",
		"@types/surma__rollup-plugin-off-main-thread": "^2.2.3",
		"@urql/exchange-retry": "^1.3.0",
		"@urql/vue": "^1.4.1",
		"@xstate/react": "^4.1.3",
		"@xstate/store": "^2.6.1",
		"comlink": "^4.4.2",
		"gql.tada": "^1.8.9",
		"o1js": "1.7.0",
		"urql": "^4.1.0",
		"vue": "^3.5.11",
		"xstate": "^5.18.2"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"unbuild": "^2.0.0"
	},
	"peerDependencies": {
		"typescript": "^5.6.3"
	}
}
